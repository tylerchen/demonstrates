		<module>tapestry</module>
		<module>spring</module>
		<module>guice</module>
		<module>utils</module>
		<module>rule</module>
		<module>observer</module>
		<module>excel</module>
		<module>script</module>
		<module>spring-test</module>
		<module>db</module>

		<module>i18n</module>
		<module>cache</module>
		<module>dsmonitor</module>
		<module>querychannel</module>
		<module>dsrouter</module>
	</modules>
</project>